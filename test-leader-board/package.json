    "husky": ">=6",
    "lint-staged": ">=10",
    "postcss": "^8.3.6",
    "prettier": "2.3.2",
    "tailwindcss": "^2.2.7",
    "typescript": "4.3.5"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ],
    "*.{js,ts,jsx,tsx,md,html,json,css}": [
      "prettier --write"
    ]
